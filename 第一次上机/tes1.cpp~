#include <cstdio>
#include <cstdlib>
#include <iostream>
#include <string>
#include <sstream>

using namespace std;

int main(){
    string a;
    cin>>a;
    string b;
    cin>>b;
    stringstream ss;
    double c;
    double d;
    ss<<a;
    ss>>c;
    ss.clear();
    ss<<b;
    ss>>d;
    cout<<a<<endl;
    cout<<b<<endl;
    cout<<c<<endl;
    cout<<d<<endl;
    cout<<c*d<<endl;
    return 0;
}
