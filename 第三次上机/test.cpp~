#include <cstdio>
#include <cstdlib>
#include <iostream>

using namespace std;

int main(){
    int a;
    while(cin>>a,a>0){
        if(a==1){
            printf("a1");
        }
        else if(a==1){
            printf("aa1");
        }
        else{
            printf("else");
        }
    
    }
    return 0;
}
